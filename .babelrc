{
  "presets": [
    ["@babel/preset-env", {
      "debug": false,
      "modules": false,
      "targets": {
        "browsers": [
          "Chrome >= 52",
          "FireFox >= 44",
          "Safari >= 7",
          "ie 11",
          "last 4 Edge versions"
        ]
      },
      "useBuiltIns": "usage"
    }]
  ],
  "ignore": [
    "node_modules"
  ],
  "plugins": [
    "syntax-dynamic-import"
  ]
}