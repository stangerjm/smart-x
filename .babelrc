{
  "presets": [
    ["env", {
      "targets": {
        "browsers": [
          "Chrome >= 52",
          "FireFox >= 44",
          "Safari >= 7",
          "Explorer 11",
          "last 4 Edge versions"
        ]
      },
      "useBuiltIns": true
    }],
    "stage-2"
  ],
  "ignore": [
    "node_modules"
  ],
  "plugins": [
    "transform-class-properties",
    "syntax-class-properties"
  ]
}