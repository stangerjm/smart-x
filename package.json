{
  "name": "smart-x",
  "description": "Smart-x frontend framework",
  "version": "1.0.0",
  "author": "James Stanger",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "unit": "jest"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "gulp-v4": "^4.0.0-alpha.5",
    "vue": "^2.5.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "./node_modules/babel-jest",
      ".*\\.(vue)$": "./node_modules/vue-jest"
    },
    "snapshotSerializers": [
      "./node_modules/jest-serializer-vue"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015-ie": "^6.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "browser-sync": "^2.23.6",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "eslint": "^4.19.1",
    "file-loader": "^1.1.4",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-clean-css": "^3.9.3",
    "gulp-jsbeautifier": "^2.1.2",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^4.0.1",
    "jest": "^22.4.3",
    "jest-serializer-vue": "^1.0.0",
    "node-sass": "^4.5.3",
    "patternlab-node": "^2.12.0",
    "sass-loader": "^6.0.6",
    "styleguidekit-assets-default": "^3.5.2",
    "styleguidekit-mustache-default": "^3.1.0",
    "vue-jest": "^2.5.0",
    "vue-loader": "^13.0.5",
    "vue-server-renderer": "^2.5.16",
    "vue-template-compiler": "^2.4.4",
    "vue-test-utils": "^1.0.0-beta.11",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  }
}
