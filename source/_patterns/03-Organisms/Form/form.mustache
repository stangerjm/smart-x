<smart-form
    form-action="{{formAction}}"
    form-title="{{formTitle}}"
    :form-data="{{formData}}"
    :readonly-inputs="{{disabledInputs}}"
    form-method="{{formMethod}}"
    :empty-model="{{emptyModel}}"
    :required-inputs="['date', 'name']">
    <block-multi-select
        :options-data="{{selectData}}"
        parent-title="Type"
        child-title="Pet"
        parent-display-key="petType"
        child-display-key="petName">
    </block-multi-select>
</smart-form>